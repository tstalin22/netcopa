- parents:
  lines:
{% if 'aaa' in vars and 'authorization' in vars['aaa'] and 'commands' in vars['aaa']['authorization'] %}
{% if 'default' in vars['aaa']['authorization']['commands'] %}
  - aaa authorization commands {{ vars['aaa']['authorization']['commands']['default']['privilege_level'] }} default {{ vars['aaa']['authorization']['commands']['default']['methods']|join(' ') }}
