{% if 'activated' in vars['aaa']['authorization']['exec'] %}
  - aaa authorization commands {{ vars['aaa']['authorization']['commands']['activated']['privilege_level'] }} {{ vars['aaa']['authorization']['exec']['activated']['methods']|join(' ') }}
{% endif %}
{% endif %}
